{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "test": "node_modules/.bin/jest --watch --coverage=false",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "testDebug": "node_modules/.bin/jest -o --watch --coverage=false",
    "testFinal": "node_modules/.bin/jest",
    "updateSnapshots": "node_modules/.bin/jest -u --coverage=false",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prettier": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "dependencies": {
    "@types/jest": "^26.0.14",
    "expo": "~39.0.2",
    "expo-status-bar": "~1.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.3.tar.gz",
    "react-native-web": "~0.13.12"
  },
  "devDependencies": {
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-universe": "^5.0.0",
    "eslint-plugin-react": "^7.21.4",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "jest-expo": "^39.0.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react-native-web": "~0.13.7",
    "react-test-renderer": "^16.13.1",
    "typescript": "~3.9.5"
  },
  "private": true
}
